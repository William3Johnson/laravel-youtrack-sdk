{
    "name": "cybercog/laravel-youtrack-sdk",
    "description": "Laravel wrapper for YouTrack SDK.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "laravel",
        "youtrack",
        "yt",
        "sdk",
        "rest",
        "api",
        "client",
        "jetbrains",
        "issues",
        "bugtracker",
        "helpdesk",
        "pm",
        "tickets"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "a.komarev@cybercog.su",
            "homepage": "http://ell.folio.su",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/cybercog/laravel-youtrack-sdk",
    "support": {
        "email": "oss@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-youtrack-sdk/issues",
        "wiki": "https://github.com/cybercog/laravel-youtrack-sdk/wiki",
        "source": "https://github.com/cybercog/laravel-youtrack-sdk",
        "docs": "https://github.com/cybercog/laravel-youtrack-sdk/wiki"
    },
    "require": {
        "cybercog/youtrack-php-sdk": "^1.0",
        "illuminate/support": "~5.1.20|~5.2.0|~5.3.0|~5.4.0",
        "php": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "mockery/mockery": "^0.9.8",
        "orchestra/database": "~3.4.0",
        "orchestra/testbench": "~3.4.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Laravel\\YouTrack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Laravel\\YouTrack\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
